# Uncomment the next line to define a global platform for your project

def import_pods
  use_frameworks!
  pod 'SnapKit'
  pod 'RxSwift'
  pod 'RxCocoa'
end

target 'Evolution' do
  platform :ios, '13.1'
  import_pods
end

target 'EvolutionTests' do
  platform :ios, '13.1'
  pod 'RxBlocking'
  pod 'RxTest'
  pod 'Nimble'
end
