# Uncomment the next line to define a global platform for your project

def import_pods
  use_frameworks!
  pod 'SnapKit'
  pod 'RxSwift'
  pod 'RxCocoa'
end

target 'Evolution iOS' do
  platform :ios, '10.0'
  import_pods
end

target 'Evolution macOS' do
  platform :osx, '10.14'
  import_pods
end

